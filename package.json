{
  "name": "~~project~~",
  "contributors": [
    "{{author}}"
  ],
  "private": true,
  "version": "0.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist .webpack",
    "debug": "IS_LOCAL_DEV=true ts-node src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint --cache --fix \"./@(src|test)/**/*?(.spec).ts\" && npx prettier .eslintrc.js --write",
    "test": "jest",
    "codegen": "npm run codegen:intent",
    "codegen:intent": "node ./bin/generateIntentFile.js",
    "build": "tsc --strict",
    "local:watch": "STAGE=staging IS_LOCAL_DEV=true ts-node-dev --preserve-symlinks --watch './src/scripts/**' --inspect --files -- src/index.ts",
    "rebuild": "npm run clean && npm run build",
    "upgrade": "ncu --upgrade /^\\@volley/*/ && npm i",
    "upgrade:all": "ncu -u && npm i",
    "jovo:get:staging": "jovo get alexaSkill --stage staging --overwrite && rm -f platforms/alexaSkill/skill-package/accountLinking.json",
    "jovo:staging": "npm run codegen && npm run jovo:get:staging && jovo build --stage staging --deploy",
    "jovo:get:prod": "jovo get alexaSkill --stage dev --overwrite && rm -f platforms/alexaSkill/skill-package/accountLinking.json",
    "jovo:prod": "npm run codegen && npm run jovo:get:prod && jovo build --stage prod --deploy",
    "apply:production": "bin/deploy.sh production",
    "apply:staging": "bin/deploy.sh staging",
    "postversion": "git push && git push --tags",
    "docker:build": "docker buildx build --platform linux/amd64 . -t 375633680607.dkr.ecr.us-east-1.amazonaws.com/~~project~~:v${npm_package_version} --build-arg NPM_TOKEN=$NPM_TOKEN"
  },
